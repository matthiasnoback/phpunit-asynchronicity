{
    "name": "matthiasnoback/phpunit-asynchronicity",
    "description": "Library for asserting things that happen asynchronously with PHPUnit",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "PHPUnit",
        "asynchronicity",
        "assertion"
    ],
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "https://matthiasnoback.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Asynchronicity\\": "src/Asynchronicity/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asynchronicity\\": "tests/Asynchronicity/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "ext-pcntl": "*"
    }
}